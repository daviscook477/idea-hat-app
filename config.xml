<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.ideahatapp534065" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>idea-hat-app</name>
  <description>
        An Application for Posting and Finding Ideas
    </description>
  <author email="daviscook447@gmail.com" href="">
      Davis Cook
    </author>
  <content src="index.html"/>
  <access origin="*"/>
